#include "TimerSoftware.h"

void changeGreenFill (const int fill) {
  changePwmState(3, MAX_FILL, fill/2, MAX_FILL -( fill/2));
}

void changeRedFill (const int fill) {
  changePwmState(4, MAX_FILL, fill/2, MAX_FILL - (fill/2));
}

void changeBlueFill (const int fill) {
  changePwmState(5, MAX_FILL, fill/2, MAX_FILL - (fill/2));
}
